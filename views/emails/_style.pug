style.
    .header{
        background: rgba(67, 104, 80);
        padding: 15px;
        color: #EEF0E5;
        display: flex;
        align-items: center !important;
    }
    .container {
        color : #555335;
    }
    .logo{
        font-size: 35px;
        font-family: san-serif;
        font-weight: bold;
        color : rgba(210, 183, 116, 1);
    }
    .logo-img{
        width: 30px;
        height: 30px;
        margin-right: 10px;
        display: block;
        margin-top: auto ;
        margin-bottom: auto;
    }
    .main{
        padding: 20px;
        background-color : #EEF0E5;
        color : #555335;
        font-size: 17px;
    }
    .footer{
        display: flex;
        align-items: center !important;
        background: rgba(67, 104, 80);
        color: #EEF0E5;
        padding: 15px 10px;
    }
    .footer-link{
        display: flex;
        align-items: center;
        justify-items: center;
        margin-right: 15px;
    }
    .footer a{
        color: rgba(210, 183, 116, 1);
        display: block;
        margin: auto 0;
    }